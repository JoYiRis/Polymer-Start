{
    "presets": [
        ["env", {
            "module": false,
            "targets": {
                "browsers": [
                    "last 2 Chrome versions",
                    "last 2 Edge versions",
                    "last 2 Firefox versions",
                    "Safari >= 10",
                    "iOS >= 10"
                ]
            },
            "exclude": ["transform-es2015-modules-commonjs"]
        }],
        ["minify", {
            "keepFnName": true,
            "mergeVars": false
        }]
    ]
}
